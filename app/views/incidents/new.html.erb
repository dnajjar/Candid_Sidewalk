<h2>My Image</h2>




<%= form_for @incident do |f| %>
 <p>
   <label>My Image</label>
   <%= f.label :name %>
   <%= f.text_field :name %>
   <%= f.label :description %>
   <%= f.text_field :description %>
   <%= f.file_field :image %>
   <%= f.hidden_field :image_cache %>
   <%= f.submit  %>
 </p>
<% end %>









